{
  "extends": "../../tslint.json",
  "rules": {
    "no-bitwise": false,
    "no-string-literal": false,
    "no-inferrable-types": false,
    "no-shadowed-variable": false,
    "directive-selector": [
      true,
      "attribute",
      "ngx",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "ngx",
      "kebab-case"
    ]
  }
}
